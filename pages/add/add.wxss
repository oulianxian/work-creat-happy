/* pages/test/healthService/registerVip/registerVip.wxss */
.register-container {
  display: flex;
  flex:1;
  width: 100%;
  height:100vh;
  flex-direction: column;
  background-color: #F5F5F5;
  position: fixed;
}

.checker-view {
  background-color: #fff;
  height: 190rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
}

.photo {
  width: 128rpx;
  height: 128rpx;
  margin-left: 32rpx;
  margin-top: 30rpx;
  background-color: #444;
  border-radius: 64rpx;
}

.checker-info {
  display: flex;
  flex-direction: column;
  flex: 1;
  margin-left: 20rpx;
}

.checker-item {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.item-top {
  margin-top: 40.8rpx;
}

.item-bottom {
  margin-top: 3.6rpx;
}

.gender-icon {
  width:32rpx;
  height: 32rpx;
  margin-left: 20rpx;
  /* background-color: #444; */
}

.checker-name {
  font-family: PingFangSC-Semibold;
  font-size: 48rpx;
  line-height: 67rpx;
  color: #1A1A1A;
}

.vip-age {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #1A1A1A;
  margin-left: 17rpx;
  line-height: 37rpx;
}

.bind-title {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #B3B3B3;
  line-height: 37rpx;
  margin-top: 26rpx;
  margin-bottom: 14rpx;
  margin-left: 31rpx;
}

.form-view {
  background-color: #fff;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.form-name {
  width: 100%;
  height: 88rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

/* .form-phone {
  width: 100%;
  height: 88rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
} */

.name-view {
  width: 150rpx;
  display: flex;
  padding-left: 32rpx;
  align-items: center;
}

.form-view input {
  flex: 1;
  margin-right: 32rpx;
}

.form-view text {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #1A1A1A;
}

.place-input {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #A7A7A7;
}

.cell-line {
  height:1rpx;
  display: flex;
  flex: 1;
  margin-right: 32rpx;
  background-color: #E6E6E6;
  margin-left: 32rpx;
}

.form-submit {
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  height: 94rpx;
  margin: 32rpx;
  background-color: #28C785;
  border-radius: 10rpx;
  /* margin-right: 32rpx;
  margin-top: 32rpx;
  margin-bottom: 32rpx; */
}

.form-submit text{
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #FFFFFF;
  text-align: center;
}

.search-btn {
  background-color: #27C886;
  display: flex;
  flex: 1;
  margin-left: 32rpx;
  margin-right: 32rpx;
  margin-top: 32rpx;
  border-radius: 10rpx;
  justify-content: center;
  align-items: center;
  height: 94rpx;
}

.search-btn text {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #FFFFFF;
  text-align: center;
}